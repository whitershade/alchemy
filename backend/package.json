{
  "name": "alchemy-backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "develop": "cross-env nodemon --watch './**/*.ts' --exec 'ts-node' --files src/server.ts",
    "db:seed": "knex seed:run --knexfile src/knexfile.ts",
    "db:migrate": "knex migrate:latest --knexfile src/knexfile.ts",
    "db:rollback": "knex migrate:rollback --knexfile src/knexfile.ts",
    "db:reset": "npm run db:rollback && npm run db:migrate && npm run db:seed"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/express": "^4.16.1",
    "@types/lodash": "^4.14.132",
    "@types/sequelize": "^4.28.2",
    "app-root-path": "^2.2.1",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "install": "^0.12.2",
    "knex": "^0.16.5",
    "lodash": "^4.17.11",
    "nodemon": "^1.19.1",
    "npm": "^6.9.0",
    "pg": "^7.11.0",
    "sequelize": "^5.8.7",
    "socket.io": "^2.2.0",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1"
  }
}
