{
  "name": "alchemy-backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "node build/server.js",
    "preinstall": "npm install ../shared",
    "develop": "cross-env nodemon --watch './**/*.ts' --exec 'ts-node' --files src/server.ts",
    "db:seed": "knex seed:run --knexfile knexfile.ts",
    "db:migrate": "knex migrate:latest --knexfile knexfile.ts",
    "db:rollback": "knex migrate:rollback --knexfile knexfile.ts",
    "db:reset": "npm run db:rollback && npm run db:migrate && npm run db:seed",
    "db:prod:seed": "NODE_ENV=production npm run db:seed",
    "db:prod:migrate": "NODE_ENV=production npm run db:migrate",
    "db:prod:rollback": "NODE_ENV=production npm run db:rollback",
    "db:prod:reset": "NODE_ENV=production npm run db:reset"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/bluebird": "^3.5.27",
    "@types/express": "^4.16.1",
    "@types/lodash": "^4.14.132",
    "@types/node": "^12.0.10",
    "@types/sequelize": "^4.28.2",
    "@types/socket.io": "^2.1.2",
    "@types/validator": "^10.11.1",
    "alchemy-shared": "file:../shared",
    "app-root-path": "^2.2.1",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "install": "^0.12.2",
    "knex": "^2.3.0",
    "lodash": "^4.17.11",
    "nodemon": "^1.19.1",
    "npm": "^6.9.0",
    "pg": "^7.11.0",
    "sequelize": "^5.8.7",
    "socket.io": "^2.2.0",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1"
  }
}
